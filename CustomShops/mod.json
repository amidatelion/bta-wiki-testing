{
	"Name": "CustomShops",
	"Author": "Denadan",
	"Website": "https://github.com/BattletechModders/CustomShops",
	"Enabled": true,
	"Hidden": true,
	"DependsOn": ["IRTweaks"],
	"DLL": "CustomShops.dll",
	"DLLEntryPoint": "CustomShops.Control.Init",
	"Settings": {
		"LogLevel": "Debug",
		"AddLogPrefix": true,
		"DEBUG_FactionShopAlwaysAvaliable": false,
		"DEBUG_BlackMarketAlwaysAvaliable": false,
		"DebugType": "RefreshShop, SaveLoad, ShopInterface, TabSwitch, ShopActions",
		"SystemShop": false,
		"FactionShop": false,
		"BlackMarketShop": false,
		"BuyBackShop": false,
		"CheckFakeVehicle": false,
		"FakeVehicleTag": "fake_vehicle_chassis",
		"NoDiscountOnLoan": true,
		"AllowMultiSell" : true,
		"AllowMultiBuy" : true
	},
	"Manifest": [
		{
			"Type": "Sprite",
			"Path": "sprites"
		}
	]
}
